    "es6-shim": "^0.33.3",
    "reflect-metadata": "0.1.2",
    "rxjs": "5.0.0-beta.0",
    "zone.js": "0.5.10"
  },
    "devDependencies": {
      "grunt": "^0.4.5",
      "grunt-contrib-compass": "^1.0.4",
      "grunt-contrib-sass": "^0.9.2",
      "grunt-contrib-watch": "^0.6.1",
      "concurrently": "^1.0.0",
      "live-server": "^0.9.0",
      "typescript": "^1.7.3"
  },
